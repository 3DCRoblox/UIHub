UIAspectRatioConstraint.Parent = UIHub_2
UIAspectRatioConstraint.AspectRatio = 1.500
UIAspectRatioConstraint.AspectType = Enum.AspectType.ScaleWithParentSize

for _, Option in pairs(Options_2:GetChildren()) do
	if Option:IsA("ImageLabel") then
		Option.Text.MouseButton1Click:Connect(function()
			local ActiveFrame = UIHub_2:WaitForChild(Option.Text.."Frame")
			ActiveFrame.Visible = true
			for _, Frame in pairs(UIHub_2:GetChildren()) do
				if Frame:IsA("ImageLabel") and Frame.Name ~= "Options" and Frame.Name ~= ActiveFrame.Name then
					Frame.Visible = false
				end
			end
		end)
	end
end

Text_3.MouseButton1Click:Connect(function()
	loadstring(game:HttpGet("https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source"))()
	SendNotification(Text_3)
end)

Text_4.MouseButton1Click:Connect(function()
	loadstring(game:HttpGet("https://pastebin.com/raw/1piaHBqj"))()
	SendNotification(Text_4)
end)

