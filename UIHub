end)

--Sliders

local Hold = false
local Hold1 = false

local WalkSpeedPX = WalkSpeed.AbsolutePosition.X
local WalkSpeedSX = WalkSpeed.AbsoluteSize.X
local JumpPowerPX = JumpPower.AbsolutePosition.X
local JumpPowerSX = JumpPower.AbsoluteSize.X

Text_7.Changed:Connect(function()
	local Character = Player.Character or Player.CharacterAdded:Wait()
	local Humanoid = Character:WaitForChild("Humanoid")
	local Number = tonumber(math.floor(Fill.Size.X.Scale * 275)) 
	local Number1 = tonumber(math.floor(Fill_2.Size.X.Scale * 725)) 

	Humanoid.UseJumpPower = true
	if Number then
		Humanoid.WalkSpeed = Number
		Humanoid.JumpPower = Number1
	end
end)

Text_8.Changed:Connect(function()
	local Character = Player.Character or Player.CharacterAdded:Wait()
	local Humanoid = Character:WaitForChild("Humanoid")
	local Number = tonumber(math.floor(Fill.Size.X.Scale * 275)) 
	local Number1 = tonumber(math.floor(Fill_2.Size.X.Scale * 725)) 

	Humanoid.UseJumpPower = true
	if Number then
		Humanoid.WalkSpeed = Number
		Humanoid.JumpPower = Number1
	end
end)

-- WalkSpeed

WalkSpeed.MouseButton1Down:Connect(function()
	Hold = true
	Fill.Size = UDim2.new(math.clamp((Mouse.X - WalkSpeedPX)/WalkSpeedSX,0,1),0,1,0)
	Text_7.Text = "WalkSpeed: "..math.floor(Fill.Size.X.Scale * 275)
end)

Mouse.Move:Connect(function()
	if Hold then
		Fill.Size = UDim2.new(math.clamp((Mouse.X - WalkSpeedPX)/WalkSpeedSX,0,1),0,1,0)
		Text_7.Text = "WalkSpeed: "..math.floor(Fill.Size.X.Scale * 275)
	end
end)

--JumpPower

JumpPower.MouseButton1Down:Connect(function()
	Hold1 = true
	Fill_2.Size = UDim2.new(math.clamp((Mouse.X - WalkSpeedPX)/WalkSpeedSX,0,1),0,1,0)
	Text_8.Text = "JumpPower: "..math.floor(Fill_2.Size.X.Scale * 725)
end)

UserInputService.InputEnded:Connect(function(Input, GameProccess)
	if Input.UserInputType == Enum.UserInputType.MouseButton1 then
		Hold = false
		Hold1 = false
	end
end)

Mouse.Move:Connect(function()
	if Hold1 then
		Fill_2.Size = UDim2.new(math.clamp((Mouse.X - WalkSpeedPX)/WalkSpeedSX,0,1),0,1,0)
		Text_8.Text = "JumpPower: "..math.floor(Fill_2.Size.X.Scale * 725)
	end
end)

--Drag Effect

local DragToggle = nil
local DragStart = nil
local StartPos = nil

local function UpdateInput(Input)
	local Delta = Input.Position - DragStart
	local Position = UDim2.new(StartPos.X.Scale, StartPos.X.Offset + Delta.X,
		StartPos.Y.Scale, StartPos.Y.Offset + Delta.Y)
	TweenService:Create(UIHub_2, TweenInfo.new(0), {Position = Position}):Play()
end

UIHub_2.InputBegan:Connect(function(Input)
	if (Input.UserInputType == Enum.UserInputType.MouseButton1 or Input.UserInputType == Enum.UserInputType.Touch) then 
		DragToggle = true
		DragStart = Input.Position
		StartPos = UIHub_2.Position
		Input.Changed:Connect(function()
			if Input.UserInputState == Enum.UserInputState.End then
				DragToggle = false
			end
		end)
	end
end)

UserInputService.InputChanged:Connect(function(Input)
	if Input.UserInputType == Enum.UserInputType.MouseMovement or Input.UserInputType == Enum.UserInputType.Touch then
		if DragToggle then
			UpdateInput(Input)
		end
	end
end)
